{
  "version": 2,
  "name": "fitness-os",
  "builds": [
    {
      "src": "backend/backend.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/backend.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}
